                rock.position.set(
                    Math.cos(angle) * 7.5,
                    0.3,
                    Math.sin(angle) * 7.5
                );
                rock.rotation.set(
                    Math.random() * Math.PI,
                    Math.random() * Math.PI,
                    Math.random() * Math.PI
                );
                pond.add(rock);
