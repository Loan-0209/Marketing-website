            }
            
            console.error('‚ùå ERROR:', message);
        }
        
        // ENHANCED: Promise-based THREE.js loader with multiple fallbacks
        function ensureThreeJSLoaded() {
            return new Promise((resolve, reject) => {
                console.log('üîÑ Ensuring THREE.js is loaded...');
                
                // If THREE.js is already available
                if (typeof THREE !== 'undefined') {
                    console.log('‚úÖ THREE.js already available');
                    resolve(THREE);
                    return;
                }
                
                // Try loading from multiple CDNs in sequence
                const cdnUrls = [
                    'https://cdnjs.cloudflare.com/ajax/libs/three.js/r128/three.min.js',
                    'https://unpkg.com/three@0.128.0/build/three.min.js',
                    'https://cdn.jsdelivr.net/npm/three@0.128.0/build/three.min.js',
                    'https://threejs.org/build/three.min.js'
                ];
                
                let currentCdnIndex = 0;
                
                function tryLoadFromCDN() {
                    if (currentCdnIndex >= cdnUrls.length) {
                        reject(new Error('All THREE.js CDN attempts failed'));
                        return;
                    }
                    
                    const script = document.createElement('script');
                    script.src = cdnUrls[currentCdnIndex];
                    
                    // Add timeout for each CDN attempt (5 seconds)
                    const timeoutId = setTimeout(() => {
                        console.error(`‚ùå Timeout loading THREE.js from CDN ${currentCdnIndex + 1}`);
                        script.remove();
                        currentCdnIndex++;
                        tryLoadFromCDN();
                    }, 5000);
                    
                    script.onload = () => {
                        clearTimeout(timeoutId);
                        
                        if (typeof THREE !== 'undefined') {
                            console.log(`‚úÖ THREE.js loaded from CDN ${currentCdnIndex + 1}: ${cdnUrls[currentCdnIndex]}`);
                            console.log(`üì¶ THREE.js Version: ${THREE.REVISION}`);
                            
                            // Test basic functionality
                            try {
                                const testScene = new THREE.Scene();
                                console.log('‚úÖ THREE.js functionality test passed');
                                resolve(THREE);
                            } catch (testError) {
                                console.error('‚ùå THREE.js functionality test failed:', testError);
                                reject(new Error('THREE.js loaded but not functional'));
                            }
                        } else {
                            console.error(`‚ùå THREE.js script loaded but THREE is undefined from CDN ${currentCdnIndex + 1}`);
                            currentCdnIndex++;
                            tryLoadFromCDN();
                        }
                    };
                    
                    script.onerror = () => {
                        clearTimeout(timeoutId);
                        console.error(`‚ùå Failed to load THREE.js from CDN ${currentCdnIndex + 1}: ${cdnUrls[currentCdnIndex]}`);
                        currentCdnIndex++;
                        tryLoadFromCDN();
                    };
                    
                    console.log(`üîÑ Attempting to load THREE.js from CDN ${currentCdnIndex + 1}...`);
                    document.head.appendChild(script);
                }
                
                tryLoadFromCDN();
            });
        }
        
