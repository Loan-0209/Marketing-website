                            showError('WebGL recovery failed. Please reload the page.');
                        }
                    });
                    
                    // Memory pressure handling
                    if ('memory' in performance) {
                        setInterval(() => {
                            const memInfo = performance.memory;
                            const usage = memInfo.usedJSHeapSize / memInfo.jsHeapSizeLimit;
                            
                            if (usage > 0.85) {
