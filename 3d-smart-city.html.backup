                            shadows: capabilities.extensions['WEBGL_depth_texture'],
                            antialiasing: true,
                            postProcessing: capabilities.extensions['WEBGL_draw_buffers'],
                            particleCount: 3000,
                            textureQuality: 'high',
                            geometryDetail: 'medium',
