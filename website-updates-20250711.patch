+        color: #FFA726; 
+        text-shadow: 0 2px 4px rgba(0,0,0,0.5);
+    }
+    50% { 
+        color: #FFB74D; 
+        text-shadow: 
+            0 2px 4px rgba(0,0,0,0.5),
+            0 0 20px rgba(255,167,38,0.8);
+    }
+}
+
+/* Subtle Surface Texture */
+.building-facade::before {
+    content: '';
+    position: absolute;
+    top: 0;
+    left: 0;
+    right: 0;
+    bottom: 0;
+    background: 
