+    
+    .intro-text {
+        font-size: 1rem;
+    }
+    
+    .section-title {
+        font-size: 1.3rem;
+    }
+    
+    .building {
+        width: 160px;
+        height: 240px;
+    }
+    
+    .content-column {
+        padding: 20px 15px;
+    }
+}
