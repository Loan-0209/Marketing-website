+<!DOCTYPE html>
+<html lang="en">
+<head>
+    <meta charset="UTF-8">
+    <meta name="viewport" content="width=device-width, initial-scale=1.0">
+    <title>Minimal AI Campus 3D</title>
+    
+    <!-- Three.js v√† c√°c th∆∞ vi·ªán li√™n quan -->
+    <script src="https://cdn.jsdelivr.net/npm/three@0.128.0/build/three.min.js"></script>
+    <script src="https://cdn.jsdelivr.net/npm/three@0.128.0/examples/js/controls/OrbitControls.js"></script>
+    <script src="https://cdn.jsdelivr.net/npm/three@0.128.0/examples/js/libs/stats.min.js"></script>
+    
+    <style>
+        body {
+            margin: 0;
+            padding: 0;
+            overflow: hidden;
+            background-color: #000;
+        }
+        #info {
+            position: absolute;
+            top: 10px;
+            left: 10px;
+            color: white;
+            font-family: Arial, sans-serif;
+            background-color: rgba(0, 0, 0, 0.7);
+            padding: 10px;
+            border-radius: 5px;
+        }
+    </style>
+</head>
+<body>
+    <div id="info">Minimal AI Campus 3D</div>
+    
+    <script>
+        // Bi·∫øn to√†n c·ª•c
+        let scene, camera, renderer, controls;
+        let stats;
+        let clock;
+        
+        // Kh·ªüi t·∫°o
+        init();
+        animate();
+        
+        function init() {
+            // Kh·ªüi t·∫°o clock
+            clock = new THREE.Clock();
+            
+            // T·∫°o scene
+            scene = new THREE.Scene();
+            scene.background = new THREE.Color(0x000000);
+            
+            // T·∫°o camera
+            camera = new THREE.PerspectiveCamera(60, window.innerWidth / window.innerHeight, 0.1, 1000);
+            camera.position.set(20, 20, 20);
+            camera.lookAt(0, 0, 0);
+            
+            // T·∫°o renderer
+            renderer = new THREE.WebGLRenderer({ antialias: true });
+            renderer.setSize(window.innerWidth, window.innerHeight);
+            renderer.shadowMap.enabled = true;
+            document.body.appendChild(renderer.domElement);
+            
+            // T·∫°o controls
+            controls = new THREE.OrbitControls(camera, renderer.domElement);
+            controls.enableDamping = true;
+            controls.dampingFactor = 0.05;
+            
+            // T·∫°o stats
+            stats = new Stats();
+            document.body.appendChild(stats.dom);
+            
+            // Th√™m √°nh s√°ng
+            const ambientLight = new THREE.AmbientLight(0x404040, 0.5);
+            scene.add(ambientLight);
+            
+            const directionalLight = new THREE.DirectionalLight(0xffffff, 1);
+            directionalLight.position.set(10, 20, 10);
+            directionalLight.castShadow = true;
+            scene.add(directionalLight);
+            
+            // T·∫°o m·∫∑t ƒë·∫•t
+            const groundGeometry = new THREE.PlaneGeometry(50, 50);
+            const groundMaterial = new THREE.MeshStandardMaterial({
+                color: 0x333333,
+                roughness: 0.8,
+                metalness: 0.2
+            });
+            const ground = new THREE.Mesh(groundGeometry, groundMaterial);
+            ground.rotation.x = -Math.PI / 2;
+            ground.receiveShadow = true;
+            scene.add(ground);
+            
+            // T·∫°o grid helper
+            const gridHelper = new THREE.GridHelper(50, 50, 0x555555, 0x333333);
+            scene.add(gridHelper);
+            
+            // T·∫°o t√≤a nh√† k√≠nh cobalt blue
+            createGlassBuilding(0, 0, 0, 10, 20, 10, 0x0047AB);
+            createGlassBuilding(-15, 0, -15, 8, 15, 8, 0x0055CC);
+            createGlassBuilding(15, 0, -15, 8, 25, 8, 0x0066DD);
+            createGlassBuilding(-15, 0, 15, 8, 18, 8, 0x0077EE);
+            createGlassBuilding(15, 0, 15, 8, 12, 8, 0x0088FF);
+            
+            // X·ª≠ l√Ω s·ª± ki·ªán resize
+            window.addEventListener('resize', onWindowResize, false);
+        }
+        
+        function createGlassBuilding(x, y, z, width, height, depth, color) {
+            // T·∫°o v·∫≠t li·ªáu k√≠nh
+            const glassMaterial = new THREE.MeshPhysicalMaterial({
+                color: color,
+                transparent: true,
+                opacity: 0.7,
+                metalness: 0.2,
+                roughness: 0.05,
+                transmission: 0.95,
+                reflectivity: 1.0,
+                clearcoat: 1.0,
+                clearcoatRoughness: 0.1
+            });
+            
+            // T·∫°o kh·ªëi ch√≠nh c·ªßa t√≤a nh√†
+            const buildingGeometry = new THREE.BoxGeometry(width, height, depth);
+            const building = new THREE.Mesh(buildingGeometry, glassMaterial);
+            building.position.set(x, y + height / 2, z);
+            building.castShadow = true;
+            building.receiveShadow = true;
+            scene.add(building);
+            
+            return building;
+        }
+        
+        function onWindowResize() {
+            camera.aspect = window.innerWidth / window.innerHeight;
+            camera.updateProjectionMatrix();
+            renderer.setSize(window.innerWidth, window.innerHeight);
+        }
+        
+        function animate() {
+            requestAnimationFrame(animate);
+            
+            const delta = clock.getDelta();
+            
+            // C·∫≠p nh·∫≠t controls
+            controls.update();
+            
+            // C·∫≠p nh·∫≠t stats
+            stats.update();
+            
+            // Render scene
+            renderer.render(scene, camera);
+        }
+    </script>
+    
+    <!-- T√≠ch h·ª£p force-render-loop.js ƒë·ªÉ ƒë·∫£m b·∫£o render li√™n t·ª•c -->
+    <script src="force-render-loop.js"></script>
