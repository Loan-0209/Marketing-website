+
+/* Row 9: 3 windows */
+.window-exact:nth-child(33) { background: transparent; }
+.window-exact:nth-child(34) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(35) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(36) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+
+/* Row 10: 4 windows (all) */
+.window-exact:nth-child(37) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(38) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(39) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(40) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+
+/* Exact Pattern Animation */
+@keyframes windows-exact-glow {
+    0%, 100% { 
+        opacity: 0.9;
