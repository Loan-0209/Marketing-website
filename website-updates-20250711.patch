+        this.state.building.style.backfaceVisibility = 'hidden';
+        this.state.building.style.perspective = '1000px';
+    },
+    
+    // Setup window flicker animations
+    setupWindowAnimations: function() {
+        this.state.windows.forEach((window, index) => {
