+// H·ªá th·ªëng ph·∫£n chi·∫øu v√† hi·ªáu ·ª©ng m√¥i tr∆∞·ªùng cho AI Campus 3D
+function createReflectionSystem(scene, renderer, camera) {
+    const reflectionSystem = {
+        cubeCamera: null,
+        cubeRenderTarget: null,
+        update: null
+    };
+    
+    // T·∫°o cube render target v·ªõi ƒë·ªô ph√¢n gi·∫£i cao
+    const cubeRenderTarget = new THREE.WebGLCubeRenderTarget(512, {
+        format: THREE.RGBFormat,
+        generateMipmaps: true,
+        minFilter: THREE.LinearMipmapLinearFilter
+    });
+    
+    // T·∫°o cube camera
+    const cubeCamera = new THREE.CubeCamera(0.1, 1000, cubeRenderTarget);
+    scene.add(cubeCamera);
+    
+    // L∆∞u tr·ªØ c√°c tham chi·∫øu
+    reflectionSystem.cubeCamera = cubeCamera;
+    reflectionSystem.cubeRenderTarget = cubeRenderTarget;
+    
+    // H√†m c·∫≠p nh·∫≠t ph·∫£n chi·∫øu
+    reflectionSystem.update = function(deltaTime) {
+        // C·∫≠p nh·∫≠t v·ªã tr√≠ c·ªßa cube camera
+        this.cubeCamera.position.set(0, 10, 0);
+        
+        // ·∫®n t·∫°m th·ªùi c√°c ƒë·ªëi t∆∞·ª£ng kh√¥ng c·∫ßn ph·∫£n chi·∫øu
+        const objectsToHide = findObjectsByType(scene, 'Mesh', ['LED', 'Ground']);
+        objectsToHide.forEach(obj => {
+            obj.visible = false;
+        });
+        
+        // C·∫≠p nh·∫≠t cube camera
+        this.cubeCamera.update(renderer, scene);
+        
+        // Hi·ªán l·∫°i c√°c ƒë·ªëi t∆∞·ª£ng ƒë√£ ·∫©n
+        objectsToHide.forEach(obj => {
+            obj.visible = true;
+        });
+        
+        // √Åp d·ª•ng environment map cho t·∫•t c·∫£ c√°c v·∫≠t li·ªáu k√≠nh
+        const glassMaterials = findMaterialsByType(scene, 'MeshPhysicalMaterial');
+        glassMaterials.forEach(material => {
+            material.envMap = this.cubeRenderTarget.texture;
+            material.envMapIntensity = 1.0;
+            material.needsUpdate = true;
+        });
+    };
+    
+    return reflectionSystem;
+}
+
+// T√¨m c√°c ƒë·ªëi t∆∞·ª£ng theo lo·∫°i
+function findObjectsByType(scene, type, excludeNames = []) {
+    const objects = [];
+    
+    scene.traverse(function(object) {
+        if (object.type === type) {
+            // Ki·ªÉm tra xem t√™n ƒë·ªëi t∆∞·ª£ng c√≥ n·∫±m trong danh s√°ch lo·∫°i tr·ª´ kh√¥ng
+            let exclude = false;
+            for (const name of excludeNames) {
+                if (object.name.includes(name)) {
+                    exclude = true;
+                    break;
+                }
+            }
+            
+            if (!exclude) {
+                objects.push(object);
+            }
+        }
+    });
+    
+    return objects;
+}
+
+// T√¨m c√°c v·∫≠t li·ªáu theo lo·∫°i
+function findMaterialsByType(scene, materialType) {
+    const materials = [];
+    
+    scene.traverse(function(object) {
+        if (object.material) {
+            if (Array.isArray(object.material)) {
+                object.material.forEach(mat => {
+                    if (mat.type === materialType) {
+                        materials.push(mat);
+                    }
+                });
+            } else if (object.material.type === materialType) {
+                materials.push(object.material);
+            }
+        }
+    });
+    
+    return materials;
+}
