+/* Random Windows Light Effects */
+.windows-container::before {
+    content: '';
+    position: absolute;
+    top: 0;
+    left: 0;
+    right: 0;
+    bottom: 0;
+    background: 
+        /* Subtle light effects for windows */
+        radial-gradient(circle at 15% 25%, rgba(255,255,255,0.3) 6px, transparent 8px),
