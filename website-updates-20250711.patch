+        linear-gradient(to right, transparent 15%, #FFA726 15%, #FFA726 25%, transparent 25%),
+        linear-gradient(to right, transparent 55%, #FFD54F 55%, #FFD54F 65%, transparent 65%),
+        linear-gradient(to right, transparent 85%, #FFA726 85%, #FFA726 95%, transparent 95%),
+        
+        linear-gradient(to right, transparent 25%, #FFD54F 25%, #FFD54F 35%, transparent 35%),
+        linear-gradient(to right, transparent 60%, #FFA726 60%, #FFA726 70%, transparent 70%),
