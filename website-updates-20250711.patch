+            });
+            
+            // Lan can b√™n tr√°i
+            const leftRailingGeometry = new THREE.BoxGeometry(length, 0.5, 0.2);
+            const leftRailing = new THREE.Mesh(leftRailingGeometry, railingMaterial);
+            leftRailing.position.set(0, height / 2 + 0.25, width / 2 - 0.1);
