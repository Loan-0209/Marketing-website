+    const light = new THREE.Mesh(lightGeometry, glowMaterial);
+    light.position.y = height * 0.4 + height * 0.15 + height * 0.15;
+    group.add(light);
+    
+    // Vòng phát sáng
+    const ringGeometry = new THREE.TorusGeometry(width/2.5, 0.3, 16, 32);
