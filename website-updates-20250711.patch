+    border-radius: 8px 8px 0 0;
+}
+
+/* Building Subtitle */
+.building-subtitle-exact {
+    position: absolute;
+    top: 48px;
+    left: 0;
+    right: 0;
+    background: white;
+    color: #5A67D8;
+    font-size: 11px;
+    font-weight: 600;
