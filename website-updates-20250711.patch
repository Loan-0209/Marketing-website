+/* HEART Technology Park - Advanced Parallax System */
+
+// Parallax Manager
+window.HEARTParallax = {
+    // Configuration
+    config: {
+        buildingSpeed: 0.3,
+        windowFlickerRate: 3000,
+        floatAmplitude: 8,
+        rotationMax: 3,
+        scaleRange: 0.05,
+        smoothing: 0.1
