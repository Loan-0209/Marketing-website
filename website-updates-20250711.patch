+}
+
+.windows {
+    position: absolute;
+    top: 80px;
+    left: 20px;
+    right: 20px;
+    bottom: 20px;
+    background: 
+        /* Rectangle windows pattern */
+        linear-gradient(to right, transparent 10%, #FFD54F 10%, #FFD54F 20%, transparent 20%),
+        linear-gradient(to right, transparent 40%, #FFA726 40%, #FFA726 50%, transparent 50%),
+        linear-gradient(to right, transparent 70%, #FFD54F 70%, #FFD54F 80%, transparent 80%),
