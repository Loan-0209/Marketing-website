-    <!-- Parallax Scroll Effect -->
-    <script>
-    // Performance optimized parallax effect with Intersection Observer
-    function initParallaxEffect() {
-        console.log("=== INIT PARALLAX EFFECT ===");
-        const parallaxElement = document.getElementById('buildingParallax');
-        const container = document.querySelector('.parallax-container');
-        
-        console.log("Parallax element found:", parallaxElement);
-        console.log("Container found:", container);
-        
-        if (!parallaxElement || !container) {
-            console.log("Missing elements - parallax will not work");
-            return;
-        }
-        
-        let isInView = false;
-        let ticking = false;
-        
-        // Intersection Observer to check if element is in viewport
-        const observer = new IntersectionObserver((entries) => {
-            entries.forEach(entry => {
-                isInView = entry.isIntersecting;
-            });
-        }, {
-            threshold: 0,
-            rootMargin: '100px'
-        });
-        
-        observer.observe(container);
-        
-        // Simplified parallax function
-        function updateParallax() {
-            if (!isInView) {
-                ticking = false;
-                return;
-            }
-            
-            const scrolled = window.pageYOffset;
-            const parallaxSpeed = 0.3; // Simple multiplier
-            const yPos = scrolled * parallaxSpeed;
-            
-            console.log("Parallax update - Scroll:", scrolled, "YPos:", yPos);
-            
-            // Apply simple transform
-            parallaxElement.style.transform = `translateY(${yPos}px)`;
-            
-            ticking = false;
-        }
-        
-        function requestTick() {
-            if (!ticking) {
-                requestAnimationFrame(updateParallax);
-                ticking = true;
-            }
-        }
-        
-        // Listen for scroll events with passive option for better performance
-        window.addEventListener('scroll', requestTick, { passive: true });
-        
-        // Initial update
-        updateParallax();
-        
-        // Update on resize
-        window.addEventListener('resize', () => {
-            requestTick();
-        }, { passive: true });
-    }
-
-    // Initialize when DOM is loaded
-    document.addEventListener('DOMContentLoaded', initParallaxEffect);
-
-    // Also initialize if DOM is already loaded
-    if (document.readyState === 'loading') {
