+            '.ttf': 'font/ttf',
+            '.otf': 'font/otf',
+            '.eot': 'application/vnd.ms-fontobject'
+        }
+        
+        if ext.lower() in mime_types:
+            return mime_types[ext.lower()], None
+        
+        return super().guess_type(path)
