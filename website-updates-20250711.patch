+        radial-gradient(circle at 45% 85%, #8E24AA 8px, transparent 8px),
+        radial-gradient(circle at 75% 85%, #FFA726 8px, transparent 8px);
+    background-size: 100% 100%;
+    border-radius: 10px;
+    opacity: 0.8;
+    animation: windows-glow 4s ease-in-out infinite;
+}
+
+/* Random Windows Animation */
+@keyframes windows-glow {
+    0%, 100% { 
+        opacity: 0.8;
+        filter: brightness(1);
+    }
+    25% { 
+        opacity: 0.9;
+        filter: brightness(1.1);
+    }
+    50% { 
+        opacity: 1;
+        filter: brightness(1.2);
+    }
+    75% { 
+        opacity: 0.95;
+        filter: brightness(1.1);
+    }
