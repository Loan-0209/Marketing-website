+    border-radius: 0 15px 0 0;
+    box-shadow: inset 4px 0 8px rgba(0,0,0,0.3);
+}
+
+/* Building Roof */
+.building-roof {
+    position: absolute;
+    width: 115%;
+    height: 12%;
+    top: -12%;
