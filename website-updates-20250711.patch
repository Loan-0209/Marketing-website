+
+/* Container */
+.container {
+    max-width: 1200px;
+    margin: 0 auto;
+    padding: 0 20px;
+}
+
+/* Main Layout */
+.main-layout {
+    display: grid;
+    grid-template-columns: 1fr 33.33%;
+    min-height: 100vh;
+    gap: 0;
+}
+
+/* Content Column */
+.content-column {
+    padding: 60px 40px 60px 60px;
+    background: #ffffff;
+}
+
+/* Building Column */
+.building-column {
+    position: relative;
+    background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
