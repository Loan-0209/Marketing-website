+    overflow: hidden;
+}
+
+/* Building Side Panel (Right) */
+.building-side {
+    position: absolute;
+    width: 15%;
+    height: 100%;
+    right: -15%;
+    top: 0;
+    background: 
+        linear-gradient(180deg, rgba(255,255,255,0.1) 0%, transparent 50%),
+        linear-gradient(90deg, #5A5FCF 0%, #6B46C1 50%, #553C9A 100%);
+    transform: rotateY(90deg);
