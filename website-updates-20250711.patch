+        0 5%, 0 5%, 0 5%,
+        /* Row 2 */
+        0 15%, 0 15%, 0 15%, 0 15%,
+        /* Row 4 */
+        0 35%, 0 35%,
+        /* Row 5 */
+        0 45%, 0 45%, 0 45%,
+        /* Row 7 */
+        0 65%, 0 65%, 0 65%, 0 65%,
+        /* Row 8 */
