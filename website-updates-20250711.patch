+.window-exact:nth-child(17) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(18) { background: transparent; }
+.window-exact:nth-child(19) { background: transparent; }
+.window-exact:nth-child(20) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+
+/* Row 6: Empty */
+.window-exact:nth-child(21) { background: transparent; }
+.window-exact:nth-child(22) { background: transparent; }
+.window-exact:nth-child(23) { background: transparent; }
+.window-exact:nth-child(24) { background: transparent; }
+
+/* Row 7: 4 windows (all) */
+.window-exact:nth-child(25) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(26) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(27) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(28) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+
+/* Row 8: 2 windows (sides) */
+.window-exact:nth-child(29) { background: linear-gradient(135deg, #FFD54F 0%, #FFA726 100%); }
+.window-exact:nth-child(30) { background: transparent; }
+.window-exact:nth-child(31) { background: transparent; }
