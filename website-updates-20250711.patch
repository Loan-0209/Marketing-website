+}
+
+/* Exact Windows Container - Positioned Layout */
+.windows-exact-pattern {
+    position: absolute;
+    top: 80px;
+    left: 20px;
+    right: 20px;
+    bottom: 20px;
+    /* Exact pattern using multiple backgrounds positioned precisely */
+    background: 
+        /* Row 1: 3 windows (evenly spaced, sides empty) */
+        linear-gradient(to right, transparent 12.5%, #FFD54F 12.5%, #FFA726 25%, transparent 25%),
+        linear-gradient(to right, transparent 37.5%, #FFD54F 37.5%, #FFA726 50%, transparent 50%),
+        linear-gradient(to right, transparent 62.5%, #FFD54F 62.5%, #FFA726 75%, transparent 75%),
+        
+        /* Row 2: 4 windows (full row) */
+        linear-gradient(to right, #FFD54F 0%, #FFA726 12.5%, transparent 12.5%),
+        linear-gradient(to right, transparent 25%, #FFD54F 25%, #FFA726 37.5%, transparent 37.5%),
+        linear-gradient(to right, transparent 50%, #FFD54F 50%, #FFA726 62.5%, transparent 62.5%),
+        linear-gradient(to right, transparent 75%, #FFD54F 75%, #FFA726 100%),
+        
+        /* Row 3: Empty (no background needed) */
+        
+        /* Row 4: 2 windows (center, sides empty) */
+        linear-gradient(to right, transparent 25%, #FFD54F 25%, #FFA726 37.5%, transparent 37.5%),
+        linear-gradient(to right, transparent 62.5%, #FFD54F 62.5%, #FFA726 75%, transparent 75%),
+        
+        /* Row 5: 3 windows (left, center empty, right) */
+        linear-gradient(to right, #FFD54F 0%, #FFA726 12.5%, transparent 12.5%),
+        linear-gradient(to right, transparent 43.75%, #FFD54F 43.75%, #FFA726 56.25%, transparent 56.25%),
+        linear-gradient(to right, transparent 87.5%, #FFD54F 87.5%, #FFA726 100%),
+        
+        /* Row 6: Empty (no background needed) */
+        
+        /* Row 7: 4 windows (full row) */
+        linear-gradient(to right, #FFD54F 0%, #FFA726 12.5%, transparent 12.5%),
+        linear-gradient(to right, transparent 25%, #FFD54F 25%, #FFA726 37.5%, transparent 37.5%),
+        linear-gradient(to right, transparent 50%, #FFD54F 50%, #FFA726 62.5%, transparent 62.5%),
+        linear-gradient(to right, transparent 75%, #FFD54F 75%, #FFA726 100%),
+        
+        /* Row 8: 2 windows (sides, center large empty) */
+        linear-gradient(to right, #FFD54F 0%, #FFA726 12.5%, transparent 12.5%),
+        linear-gradient(to right, transparent 87.5%, #FFD54F 87.5%, #FFA726 100%),
